{
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "modules": {
          "tatum-edge-module": {
            "settings": {
              "image": "PRIVATE_IMAGE_REGISTRY_HOST/tatum-edge-node-red:latest",
              "createOptions": "{\"HostConfig\":{\"Privileged\":true,\"PortBindings\":{\"1880/tcp\":[{\"HostPort\":\"1880\"}]}}}"
            },
            "type": "docker",
            "version": "1.0",
            "status": "running",
            "restartPolicy": "always"
          }
        },
        "runtime": {
          "settings": {
            "minDockerVersion": "v1.25",
            "registryCredentials": {
              "tatum-image-registry": {
                "address": "PRIVATE_IMAGE_REGISTRY_HOST",
                "password": "PRIVATE_IMAGE_REGISTRY_PASSWORD",
                "username": "PRIVATE_IMAGE_REGISTRY_USERNAME"
              }
            }
          },
          "type": "docker"
        },
        "schemaVersion": "1.1",
        "systemModules": {
          "edgeAgent": {
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.1",
              "createOptions": "{}"
            },
            "type": "docker"
          },
          "edgeHub": {
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.1",
              "createOptions": "{\"HostConfig\":{\"PortBindings\":{\"443/tcp\":[{\"HostPort\":\"443\"}],\"5671/tcp\":[{\"HostPort\":\"5671\"}],\"8883/tcp\":[{\"HostPort\":\"8883\"}]}}}"
            },
            "type": "docker",
            "status": "running",
            "restartPolicy": "always"
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "routes": {
          "route": "FROM /messages/* INTO $upstream"
        },
        "schemaVersion": "1.1",
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    },
    "tatum-edge-module": {
      "properties.desired": {}
    }
  }
}